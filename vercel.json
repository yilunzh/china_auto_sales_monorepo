{
  "framework": "nextjs",
  "buildCommand": "ls -la && find . -type d -name 'frontend' && npm run build:frontend",
  "outputDirectory": "packages/frontend/.next",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/packages/frontend/$1"
    }
  ]
} 